{
  "appid": "rff.app.bitsights",
  "appname": "BitSights",
  "name": "bitsights",
  "version": "1.0.7",
  "private": true,
  "homepage": ".",
  "repository": {
    "type": "git",
    "url": "git@github.com:RedFoxFinn/BitSights.git"
  },
  "dependencies": {
    "@date-io/dayjs": "^2.11.0",
    "@emotion/react": "^11.6.0",
    "@emotion/styled": "^11.6.0",
    "@fontsource/roboto": "^4.5.1",
    "@mui/icons-material": "^5.2.0",
    "@mui/lab": "^5.0.0-alpha.56",
    "@mui/material": "^5.2.0",
    "@reduxjs/toolkit": "^1.6.2",
    "@testing-library/jest-dom": "^5.15.1",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "axios": "^0.24.0",
    "chart.js": "^3.6.1",
    "cypress": "^9.1.0",
    "dayjs": "^1.10.7",
    "react": "^17.0.2",
    "react-chartjs-2": "^4.0.0",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.6",
    "react-scripts": "^3.4.4"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "scripts": {
    "cm": "git-cz",
    "release": "standard-version",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "test": "react-scripts test --colors --no-cache --coverage --verbose --notify --watchAll --noStackTrace --transformIgnorePatterns 'node_modules/*' ",
    "ci": "react-scripts test --colors --no-cache --coverage --verbose --noStackTrace --transformIgnorePatterns 'node_modules/*'",
    "eject": "react-scripts eject",
    "lint": "eslint --fix --ext .js,.jsx,.ts,.tsx .",
    "cy:run": "cypress run --record",
    "cy:open": "cypress open"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "airbnb",
      "airbnb/hooks"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!node_modules/",
      "!src/index.js",
      "!src/reportWebVitals.js",
      "!src/styles/*",
      "!src/controllers/redux/slices/*"
    ],
    "coverageThreshold": {
      "global": {
        "functions": 50,
        "lines": 50
      }
    },
    "coverageReporters": [
      "json-summary",
      "text",
      "lcov"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "commitizen": "^3.0.0",
    "cz-conventional-changelog": "^3.0.1",
    "gh-pages": "^3.2.3",
    "husky": "^7.0.4",
    "standard-version": "^9.3.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
